{
    "name": "cwp/cwp",
    "description": "CWP features module. We strongly recommend using it for all new sites. Future features will be delivered here.",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "cwp"
    ],
    "license": "BSD-3-Clause",
    "require": {
        "php": "^8.1",
        "silverstripe/framework": "^5",
        "cwp/cwp-core": "^3",
        "silverstripe/cms": "^5",
        "silverstripe/taxonomy": "^3",
        "symbiote/silverstripe-gridfieldextensions": "^4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3",
        "cwp/starter-theme": "^4"
    },
    "conflict": {
        "silverstripe/auditor": "<2.2.1"
    },
    "suggest": {
        "silverstripe/crontask": "Allows CWP cleanup tasks to be scheduled"
    },
    "extra": {
        "expose": [
            "css",
            "javascript",
            "images"
        ]
    },
    "autoload": {
        "psr-4": {
            "CWP\\CWP\\": "src/",
            "CWP\\CWP\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
